{
  "name": "@coding-wisely/vue-tailwind-country-city-selector",
  "version": "1.0.0",
  "description": "Country/City selector component based on Tailwind.css and Vue.js",
  "main": "dist/vue-tailwind-country-city-selector.umd.js",
  "module": "dist/vue-tailwind-country-city-selector.esm.js",
  "unpkg": "dist/vue-tailwind-country-city-selector.min.js",
  "browser": {
    "./sfc": "src/vue-tailwind-country-city-selector.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-tailwind-country-city-selector.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-tailwind-country-city-selector.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-tailwind-country-city-selector.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coding-wisely/vue-tailwind-country-city-selector.git"
  },
  "keywords": [
    "Tailwind",
    "Vue",
    "Country City selector",
    "City selector",
    "Country selector"
  ],
  "author": "Vladimir Nikolic <nezaboravi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coding-wisely/vue-tailwind-country-city-selector/issues"
  },
  "homepage": "https://github.com/coding-wisely/vue-tailwind-country-city-selector#readme",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "autoprefixer": "^9.7.4",
    "core-js": "^3.6.4",
    "postcss": "^7.0.36",
    "postcss-import": "^12.0.1",
    "rollup": "^1.29.0",
    "rollup-plugin-vue": "^5.1.5",
    "tailwind": "^4.0.0",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
